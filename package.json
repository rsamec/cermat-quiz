{
  "type": "module",
  "private": false,
  "scripts": {
    "clean": "rimraf src/.observablehq/cache",
    "build": "observable build",
    "dev": "observable preview --debug",
    "deploy": "observable deploy",
    "observable": "observable",
    "merge-pdf": "node merge-pdf.js ./generated",
    "baml-generate": "baml-cli generate",
    "build:word-problems": "esbuild ./src/math/word-problems.ts --bundle --platform=neutral --outfile=./src/math/word-problems.js",
    "build:deduce-utils": "esbuild ./src/utils/deduce-utils.ts --bundle --platform=neutral --outfile=./src/utils/deduce-utils.js",
    "build:math-solver": "esbuild ./src/utils/math-solver.ts --bundle --platform=neutral --outfile=./src/utils/math-solver.js",
    "build:tldraw": "esbuild ./src/ai/tldraw.ts --bundle --platform=neutral --outfile=./src/ai/tldraw.js",
    "build:ai": "esbuild ./src/ai/ai-utils.ts --bundle --platform=neutral --outfile=./src/ai/ai-utils.js",
    "build:schema": "esbuild ./src/ai/schema.ts --bundle --platform=neutral --outfile=./src/ai/schema.js",
    "build:all": "npm run build:math-solver && npm run build:deduce-utils && npm run build:word-problems && npm run build:schema && npm run build:ai && npm run build:tldraw",
    "generate:word-problems": "node ./src/data/word-problems-solu-[code].txt.js --code M9I-2025",
    "generate:raw": "node ./src/data/raw-[code].md.js --code M9I-2025",
    "ai:word-problems:steps": "node ./src/data/word-problems-ai-[prompt]-[code]-[model].json.js --code M9B-2025 --prompt steps --model gpt-5-mini > ./src/data/word-problems-ai-steps-M9B-2025-gpt-5-mini.json",
    "ai:word-problems:key-points": "node ./src/data/word-problems-ai-[prompt]-[code]-[model].json.js --code M9B-2025 --prompt key-points --model o4-mini > ./src/data/word-problems-ai-key-points-M9B-2025-o4-mini.json",
    "ai:word-problems:key-points:shapes": "node ./src/data/word-problems-ai-[prompt]-[code]-[model]-shapes.json.js --code M9B-2025 --prompt key-points --model o4-mini > ./src/data/word-problems-ai-key-points-M9B-2025-o4-mini-shapes.json",
    "ai:word-problems:working-sheet": "node ./src/data/word-problems-ai-[prompt]-[code]-[model].json.js --code M9B-2025 --prompt working-sheet --model o4-mini > ./src/data/word-problems-ai-working-sheet-M9B-2025-o4-mini.json",
    "ai:word-problems:working-sheet:shapes": "node ./src/data/word-problems-ai-[prompt]-[code]-[model]-shapes.json.js --code M9B-2025 --prompt working-sheet --model o4-mini > ./src/data/word-problems-ai-working-sheet-M9B-2025-o4-mini-shapes.json",
    "ai:word-problems:more-quizes": "node ./src/data/word-problems-ai-[prompt]-[code]-[model].json.js --code M9B-2025 --prompt more-quizes --model o4-mini > ./src/data/word-problems-ai-more-quizes-M9B-2025-o4-mini.json",
    "ai:word-problems:more-quizes:shapes": "node ./src/data/word-problems-ai-[prompt]-[code]-[model]-shapes.json.js --code M9B-2025 --prompt more-quizes --model o4-mini > ./src/data/word-problems-ai-more-quizes-M9B-2025-o4-mini-shapes.json",
    "generate:tldr": "node ./src/data/word-problems-[code].tldr.js --code M9I-2025 > ./src/data/word-problems-M9I-2025.tldr",
    "generate:period-4:tldr": "node ./src/data/word-problems-math-[period].tldr.js --period 4  > ./src/data/word-problems-math-4.tldr",
    "generate:period-6:tldr": "node ./src/data/word-problems-math-[period].tldr.js --period 6  > ./src/data/word-problems-math-6.tldr",
    "generate:period-8:tldr": "node ./src/data/word-problems-math-[period].tldr.js --period 8  > ./src/data/word-problems-math-8.tldr",
    "generate:page:tldr": "node ./src/data/word-problems-page-[code].tldr.js --code MMA-2025 > ./src/data/word-problems-page-MMA-2025.tldr",
    "generate:json": "node ./src/data/word-problems-[code].tldr.js --code M9I-2025 > ./src/data/word-problems-M9I-2025.json",
    "generate:word-problems:metrics": "node ./src/data/word-problems-metrics.json.js > ./src/data/word-problems-metrics.json",
    "generate:math:metrics": "node ./src/data/math-metrics.json.js > ./src/data/math-metrics.json"
  },
  "dependencies": {
    "@google/genai": "^1.10.0",
    "@lezer/common": "^1.2.2",
    "@lezer/markdown": "^1.3.1",
    "@observablehq/framework": "^1.13.2",
    "@preact/signals-core": "^1.8.0",
    "@tldraw/tldraw": "^3.14.0",
    "@vscode/markdown-it-katex": "^1.1.1",
    "convert-units": "^3.0.0-beta.7",
    "d3-dsv": "^3.0.1",
    "d3-flextree": "^2.1.2",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.1.0",
    "d3-time-format": "^4.1.0",
    "expr-eval": "^2.0.2",
    "fraction.js": "^5.2.1",
    "fractional-indexing-jittered": "^1.0.0",
    "htl": "^0.3.1",
    "invert-color": "^2.0.0",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-html5-embed": "^1.0.0",
    "markdown-it-katex": "^2.0.3",
    "markdown-it-mark": "^4.0.0",
    "markdown-it-sup": "^2.0.0",
    "openai": "^4.91.1",
    "pako": "^2.1.0",
    "playwright": "^1.49.0",
    "prosemirror-markdown": "^1.13.2",
    "prosemirror-schema-basic": "^1.2.4",
    "redux": "^5.0.1",
    "rxjs": "^7.8.1",
    "sortablejs": "^1.15.3",
    "tippy.js": "^6.3.7",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@kitschpatrol/tldraw-cli": "^4.6.41",
    "@playwright/test": "^1.49.0",
    "@types/node": "^22.7.6",
    "pdf-lib": "^1.17.1",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=18"
  }
}
